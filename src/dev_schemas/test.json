{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DDBJ JSON Schema Example",
    "properties": {
        "submitters": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    }
                },
                "required": ["name", "email"]
            }
        }
    }
}